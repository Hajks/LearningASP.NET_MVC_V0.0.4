@{
    ViewBag.Tittle = "Edit";
}

<h2>Edit albumu</h2>
<form action="/Albums/Edit" method="post">

    <label for="albumTitle">Tytuł:</label>
    <input type="text" name="AlbumTitle" id="albumTitle" />

    <label for="title">Nazwa artysty:</label>
    <input type="text" name="Artist.Name" id="artistName" />

    <label for="price">Cena:</label>
    <input type="text" name="Price" id="price" />
    
    <input type="submit" value="Wyślij" />

</form>
